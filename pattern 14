/*pattern14
1
2 1
3 2 1
4 3 2 1
5 4 3 2 1 
6 5 4 3 2 1
7 6 5 4 3 2 1
*/
import java.util.*;
public class pattern14_prasandh {
public static void main(String [] args){

 Scanner s = new Scanner (System.in);
 int n = s.nextInt();
 int j,k,m=1;
 for(int i=1;i<=n;i++)
 { 
   m=i;
   for( k=0;k<i;k++)
   {
    System.out.print("m");
    m--;
   }
  
   System.out.println(" ");
 }


}
}
