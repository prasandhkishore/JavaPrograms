/* Program-8:
A
B B
C C C
D D D D
E E E E E */
import java.util.Scanner;
public class pattern8_prasandh {
public static void main (String [] args ){

Scanner s = new Scanner (System.in);
int n = s.nextInt();
char a = 'A';
for(int i=1;i<=n;i++)
{
 for(int j=1;j<=i;j++)
  {
   System.out.print(a+" ");
   }
   a++;
 
  System.out.println();
}

}
}
