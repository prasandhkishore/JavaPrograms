/* Program-2:
7 6 5 4 3 2 1
6 5 4 3 2 1
5 4 3 2 1
4 3 2 1
3 2 1
2 1
1 */
import java.util.Scanner;
public class pattern2_prasandh {
public static void main (String [] args ){

Scanner s = new Scanner (System.in);
int n = s.nextInt();

for(int i=n;i>=1;i--)
{
 for(int j=i;j>0;j--)
  {
   System.out.print(j+" ");
   }
  System.out.println();
}

}
}
